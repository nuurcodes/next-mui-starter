{
  "extends": [
    "next/core-web-vitals",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "no-unused-vars": "error",
    "prettier/prettier": [
      "warn",
      {
        "semi": false,
        "trailingComma": "es5",
        "singleQuote": true,
        "jsxSingleQuote": true,
        "printWidth": 80
      }
    ]
  }
}
